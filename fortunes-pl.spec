install -d $RPM_BUILD_ROOT%{_datadir}/games/fortunes
for i in $COOKIES;
do
 strfile $i;
 install $i $i.dat $RPM_BUILD_ROOT%{_datadir}/games/fortunes;
done;

%clean
rm -rf $RPM_BUILD_ROOT

